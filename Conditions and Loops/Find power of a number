import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
		Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();

        int temp = 1;
        int i;
        for(i=1; n!=0; n--) {
            temp = temp * x;
        }
        System.out.println(temp);
    }	 
	}
